<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yongjie.ZhiJianSbpt.startFlow.model.UsersInRoles" table='"USERSINROLES"'>
        <id name="id" type="java.lang.Long">
            <column name='"ID"'/>
            <generator class="native">
                <param name="sequence">SEQ_USERSINROLES</param>
            </generator>
        </id>
        <property name="userId" type="java.lang.Long">
            <column name='"USERID"'/>
        </property>
        <property name="roleId" type="java.lang.Long">
            <column name='"ROLEID"'/>
        </property>
        <property name="flag" type="java.lang.Integer">
            <column name='"FLAG"'/>
        </property>
        <property name="caoR" type="string">
            <column name='"CAOR"' length="50"/>
        </property>
        <property name="caoDate" type="date">
            <column name='"CAODATE"' length="23"/>
        </property>
    </class>
</hibernate-mapping>